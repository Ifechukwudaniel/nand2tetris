/**
 * Demultiplexor.
 * {a,b} = {in,0} if sel==0
 *         {0,in} if sel==1
 */


CHIP DMux {
    IN in, sel;
    OUT a, b;

    PARTS:
    Nand(a=in, b=sel, out=notinsel);
    Nand(a=in, b=notinsel, out=notininsel);
    Nand(a=notininsel, b=notininsel, out=a);
    Nand(a=notinsel, b=notinsel, out=b);
}
